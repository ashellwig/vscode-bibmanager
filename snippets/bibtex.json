{
    "article": {
       "prefix": "article",
       "body": [
            "@article{${1:key},",
            "   title = {${2:title}},",
            "   author = {${3:author}},",
            "   journal = {${4:journal}},",
            "   year = {${5:year}}",
            "}"
       ]
    },
    "book": {
       "prefix": "book",
       "body": [
            "@book{${1:key},",
            "   title = {${2:title}},",
            "   author = {${3:author}},",
            "   journal = {${4:journal}},",
            "   year = {${5:year}}",
            "}"
       ]
    },
    "inproceedings": {
        "prefix": "inproceedings",
        "body": [
            "@inproceedings{${1:key},",
            "   title = {${2:title}},",
            "   author = {${3:author}},",
            "   journal = {${4:journal}},",
            "   year = {${5:year}}",
            "}"
        ]
    },
    "electronic": {
        "prefix": "electronic",
        "body": [
            "@electronic{${1:key},",
            "   title = {${2:title}},",
            "   author = {${3:author}},",
            "   journal = {${4:journal}},",
            "   year = {${5:year}}",
            "}"
        ]
    },
    "misc": {
        "prefix": "misc",
        "body": [
            "@misc{${1:key},",
            "   title = {${1:title},",
            "   howpublished = {$2},",
            "   note = {$3}",
            "}"
        ]
    },
    "pages":{
        "prefix":"pages",
        "body": [
            "pages = {${1}-${2}}"
        ]
    }
}